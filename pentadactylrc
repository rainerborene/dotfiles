"1.0

loadplugins '\.(js|penta)$'
group user

set hlfind
set showtabline=multitab
set showstatuslinks=command
set passkeys+=google.com/reader:jksl<CR>,gi
set passkeys+=mail.google.com:jkroae<CR>,gi

nmap -builtin <bs> <nop>
nmap -builtin j 3j
nmap -builtin k 3k
nmap -builtin ; :
nmap -builtin ) :tn<CR>
nmap -builtin ( :tp<CR>
nmap -builtin ,/ <esc>:noh<CR>

" vim: set ft=pentadactyl:
