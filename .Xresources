! better font rendering
Xft.dpi: 96
Xft.antialias: true
Xft.rgba: rgb
Xft.hinting: true
Xft.hintstyle: hintslight

! URxvt
URxvt.underlineURLs: true
URxvt.scrollBar: false
URxvt.saveLines: 0
URxvt.internalBorder: 5
URxvt.font:xft:Tewi:style=normal:pixelsize=12
URxvt.boldFont:xft:Tewi:style=bold:pixelsize=12

! Base16 Default
! Scheme: Chris Kempson (http://chriskempson.com)

#define base00 #2b303b
#define base01 #343d46
#define base02 #4f5b66
#define base03 #65737e
#define base04 #a7adba
#define base05 #c0c5ce
#define base06 #dfe1e8
#define base07 #eff1f5
#define base08 #bf616a
#define base09 #d08770
#define base0A #ebcb8b
#define base0B #a3be8c
#define base0C #96b5b4
#define base0D #8fa1b3
#define base0E #b48ead
#define base0F #ab7967

urxvt*foreground: base05
urxvt*background: base00
urxvt*cursorColor: base05
urxvt*color0: base00
urxvt*color1: base08
urxvt*color2: base0B
urxvt*color3: base0A
urxvt*color4: base0D
urxvt*color5: base0E
urxvt*color6: base0C
urxvt*color7: base05
urxvt*color8: base03
urxvt*color9: base08
urxvt*color10: base0B
urxvt*color11: base0A
urxvt*color12: base0D
urxvt*color13: base0E
urxvt*color14: base0C
urxvt*color15: base07

! Note: colors beyond 15 might not be loaded (e.g., xterm, urxvt),
! use 'shell' template to set these if necessary
urxvt*color16: base09
urxvt*color17: base0F
urxvt*color18: base01
urxvt*color19: base02
urxvt*color20: base04
urxvt*color21: base06
