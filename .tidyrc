output-html: 1
doctype: html5
wrap: 0
indent: auto
indent-spaces: 2
wrap-attributes: 0
drop-proprietary-attributes: 0
strict-tags-attributes: 0
drop-empty-elements: 0
sort-attributes: alpha
uppercase-attributes: preserve
preserve-entities: 1
show-warnings: 0
show-errors: 1
show-info: 0
show-body-only: 1
tidy-mark: 0
quiet: 1
custom-tags: blocklevel
new-blocklevel-tags: span, g, path, rect, circle, text, slot, use, image, line, transition
